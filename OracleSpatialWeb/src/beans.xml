<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="areaDaoJdbcImpl" class="de.fraunhofer.iais.spatial.dao.jdbc.AreaDaoJdbc" />
	<bean id="areaDaoIbatisImpl" class="de.fraunhofer.iais.spatial.dao.ibatis.AreaDaoIbatis" />
	<bean id="areaMgr" class="de.fraunhofer.iais.spatial.service.AreaMgr">
		<property name="areaDao" ref="areaDaoJdbcImpl" />
	</bean>
	
	<bean id="milanpolyDaoJdbcImpl" class="de.fraunhofer.iais.spatial.dao.jdbc.MilanpolyDaoJdbc" />
	<bean id="milanpolyDaoIbatisImpl" class="de.fraunhofer.iais.spatial.dao.ibatis.MilanpolyDaoIbatis" />
	<bean id="milanpolyMgr" class="de.fraunhofer.iais.spatial.service.MilanpolyMgr">	
	</bean>

</beans>